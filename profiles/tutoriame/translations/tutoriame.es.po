msgstr "El código de seguridad ingresado no es correcto."

# Privatemsg
msgid "Reply"
msgstr "Responder"

msgid "You can not reply to this conversation because all recipients are blocked."
msgstr "Ya no puedes continuar con esta conversación."

msgid "A message has been sent to !recipients."
msgstr "Se envió el mensaje a !recipients."

msgid "A message has been sent to @user."
msgstr "Se envió el mensaje a @user."

msgid "Display older messages"
msgstr "Ver mensajes anteriores"

msgid "Display newer messages"
msgstr "Ver mensajes siguientes"

msgid "!newer_link Displaying messages @from - @to of @total !older_link"
msgstr "!newer_link Mensajes @from - @to de @total !older_link"

msgid "You must include a message in your reply."
msgstr "Debes ingresar un mensaje."

